
<table class="table table-hover">
  <thead>
    <tr>
      <th>STT</th>
      <th>Imgae</th>
      <th>username</th>
      <th>age</th>
    </tr>
  </thead>
  <tbody id="tableBody">
    <% for (var i = 0; i < Users.length; i++){ %>
      <tr>
        <td><%= i + 1 %></td>
        <td style="padding: 5px;">
            <div class="img_profile">
              <img src="<%= Users[i].img %>" />
            </div>
        </td>
        <td style="color: blue;"><%= Users[i].username %></td>
        <td><%= Users[i].age %> </td>
      </tr>
    <% } %>
  </tbody>
</table>


<div id="navbar">
  <a href="/"><img src="/assets/images/Home.png"></a>
  <a href=""><img src="/assets/images/Search.png"></a>
  <a href="/order"><img src="/assets/images/Shopping Cart.png"></a>
  <a href=""><img src="/assets/images/User.png"></a>
</div>
<style>
  table th{
    text-align: center;
  }
  table td{
    text-align: center;
  }
  .img_profile{
    width:  80px;
    height: 80px;
    border-radius: 50%;
    overflow: hidden;
  }
  .img_profile img{
    width:  100%;
    height: 100%;
    object-fit: cover;
    object-position: center;
  }
</style>